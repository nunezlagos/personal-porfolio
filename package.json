{
  "name": "personal-portfolio",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev --port 3333",
    "build": "astro build",
    "preview": "astro preview",
    "db:local:create": "wrangler d1 execute personal-portfolio --local --file=./schema.sql",
    "db:local:seed": "wrangler d1 execute personal-portfolio --local --file=./seed.sql",
    "db:remote:create": "wrangler d1 execute personal-portfolio --remote --file=./schema.sql",
    "db:remote:seed": "wrangler d1 execute personal-portfolio --remote --file=./seed.sql",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.0.0",
    "astro": "^5.0.0",
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.0",
    "vitest": "^2.1.0",
    "wrangler": "^3.78.0"
  }
}
